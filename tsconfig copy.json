{
  "extends": "@bitgaming/reinv-js-config/serverless/.tsconfig.json",
  "exclude": [
    "serverless.ts",
    "node_modules"
  ],
  "compilerOptions": {
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "moduleResolution": "Node",
    "target": "ES2020",
    "types": ["node"],
    "outDir": "dist"
  }
}